{
    "env": {
        "browser": true,
		"node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
